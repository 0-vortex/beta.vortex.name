<article class="post" data-pagefind-body>
  <header class="post-header">
    <h1 class="mb-4 text-balance tracking-tight text-current text-4xl font-sans">{{ it.title }}</h1>

    {{ it.subtitle |> md }}
  </header>

  <div class="post-body">
    {{ content }}
  </div>
</article>
